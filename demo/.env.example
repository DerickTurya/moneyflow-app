# MoneyFlow - Variáveis de Ambiente (Exemplo)
# Copie este arquivo para .env e preencha com seus valores

# API URLs
API_URL=http://localhost:3000
TRACKING_API_URL=http://localhost:3000/api/events

# Demo Mode (deixar true para apresentação)
DEMO_MODE=true

# Security
SESSION_TIMEOUT_MINUTES=30
MAX_LOGIN_ATTEMPTS=5
RATE_LIMIT_WINDOW=60000

# Features (true/false)
ENABLE_GAMIFICATION=true
ENABLE_AI_RECOMMENDATIONS=true
ENABLE_VIRTUAL_CARDS=true
ENABLE_TRACKING=true

# Debug
DEBUG_MODE=false
CONSOLE_LOGS=true

# Demo User (apenas para apresentação)
DEMO_EMAIL=demo@moneyflow.com
DEMO_PASSWORD=demo123

# GitHub Pages
PUBLIC_URL=https://derickturya.github.io/moneyflow-app/demo/
